{
  "contributors": [
    "Francisco Barros <francisco.teixeira.de.barros@outlook.com> (https://github.com/FranciscoKloganB)"
  ],
  "description": "%nest-rest-starter% core API",
  "license": "UNLICENSED",
  "private": true,
  "name": "my-nest-rest-starter",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "prestart:dev": "rimraf dist",
    "dev": "npm run start:dev",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "preformat": "eslint --fix --ext js --ext ts src/ test/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ts": "tsc --project tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "docker-compose up -d pgsqldb-e2e",
    "test:e2e": "NODE_ENV=test npm-run-all typeorm:schema:drop typeorm:migration:run test:e2e:exec test:e2e:teardown --continue-on-error",
    "test:e2e:exec": "cross-env LOG_SILENT=true jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:teardown": "docker-compose stop pgsqldb-e2e && docker-compose rm -f pgsqldb-e2e",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create database/migrations/",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate database/migrations/$npm_config_name",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:show": "npm run typeorm -- migration:show",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "typeorm:schema:drop": "npm run typeorm -- schema:drop",
    "typeorm:schema:log": "npm run typeorm -- schema:log",
    "typeorm:cache:clear": "npm run typeorm -- cache:clear",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "postinstall": "husky install",
    "up": "taze major",
    "up:install": "taze major --install && npx taze -w && npm i"
  },
  "dependencies": {
    "@casl/ability": "^6.3.2",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/uuid": "^9.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "nanoid": "^4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "taze": "^0.8.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=9.1.2",
    "pnpm": "please-use-pnpm",
    "yarn": "please-use-npm"
  },
  "engineStrict": true
}
